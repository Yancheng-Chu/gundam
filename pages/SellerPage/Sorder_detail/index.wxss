page{
  background: #F7F7F7;
}
.navBar {
  width: 100%;
  height: 240rpx;
  background: #FF942A;
}

.navBar .backBtn{
  margin-top:150rpx;
  position: absolute;
  margin-left:32rpx;
}
.navBar .navTitle {
padding-top:150rpx;
font-size: 36rpx;
font-weight: 600;
color: #FFFFFF;
text-align: center;
}

.serviceCard {
  margin-top:30rpx;
  margin-left: 32rpx;
  height: 445rpx;
  width: 686rpx;
  background-color: #FFFFFF;
  border-radius: 18rpx;
  box-shadow: 0 4rpx 20rpx 0 rgba(255, 234, 234, 0.7);
  padding-top: 16rpx;
}

.serviceItem {
  margin-left: 40rpx;
  margin-right: 40rpx;
}
.serviceItem .detail{
  font-size: 28rpx;
  line-height: 70rpx;
  color: #9E9E9E;
}
.serviceItem .more{
  display: inline-flex;
  position: absolute;
  right: 80rpx;
font-size: 36rpx;
background-image:-webkit-linear-gradient(90deg,#FFBC7B,#FF8F1F,yellow); 
-webkit-background-clip:text; 
-webkit-text-fill-color:transparent;
}
.title{  
  margin-bottom:10rpx;
  line-height: 80rpx;
  color: #333333;
  font-size: 36rpx;
  font-weight: 540;
}

button {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.login {
  margin-top: 600rpx;
  width: 670rpx;
  height: 88rpx;
  border-radius: 200rpx;
  background: linear-gradient(97deg, #FFBC7B 0%, #FF8F1F 100%);
  box-shadow: 0rpx 23.04rpx 22.06rpx 0rpx rgba(21, 31, 48, 0.04);
  color: #FFFFFF;
}