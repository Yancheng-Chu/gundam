.box {
  width: 100%;
  height: 348rpx;
  background: #FFF5EB;
}

.bg {
  position: absolute;
  width: 366rpx;
  height: 282rpx;
  top: 118rpx;
  right: 0;
}

.index_title {
  position: absolute;
  width: 420rpx;
